{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:vue/essential", "google", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "plugins": ["vue", "@typescript-eslint"],
  "rules": {
    "new-cap": [
      "error",
      { "capIsNewExceptionPattern": "Component|Module|Prop|Watch" }
    ]
  }
}
